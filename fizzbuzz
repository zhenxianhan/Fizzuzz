#include<stdio.h>

int main(void){

int num;
  for(num=1;num<=100;num++){
    if((num%15==0)  
      printf("number=%d fizzbuzz\n", num);
    else if(num%3==0)
      printf("number=%d fizz\n", num);
    else if(num%5==0)
      printf("number=%d buzz\n", num);
    else
      printf("number=%d \n", num);
      }
      return 0;}
